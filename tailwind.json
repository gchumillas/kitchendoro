{
	"flex": {
		"style": {
			"display": "flex"
		}
	},
	"h-full": {
		"style": {
			"height": "100%"
		}
	},
	"w-full": {
		"style": {
			"width": "100%"
		}
	},
	"flex-row": {
		"style": {
			"flexDirection": "row"
		}
	},
	"items-center": {
		"style": {
			"alignItems": "center"
		}
	},
	"justify-center": {
		"style": {
			"justifyContent": "center"
		}
	},
	"justify-between": {
		"style": {
			"justifyContent": "space-between"
		}
	},
	"border": {
		"style": {
			"borderTopWidth": 1,
			"borderRightWidth": 1,
			"borderBottomWidth": 1,
			"borderLeftWidth": 1
		}
	},
	"bg-white": {
		"style": {
			"--tw-bg-opacity": 1,
			"backgroundColor": "rgb(255 255 255 / var(--tw-bg-opacity))"
		}
	}
}